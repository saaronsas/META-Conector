<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Identity PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Identity name="%$workgroup_name$%" workgroup="true">
    <Attributes>
        <Map>
            <entry key="displayName" value="%$workgroup_name$%"/>
        </Map>
    </Attributes>
    <AssignedScope>
        <Reference class="sailpoint.object.Scope" name="%$scope$%"/>
    </AssignedScope>
    <Capabilities>
        <Reference class="sailpoint.object.Capability" name="SHARED-Capability-Applications-READ"/>
        <Reference class="sailpoint.object.Capability" name="SHARED-Capability-Groups-READ"/>
        <Reference class="sailpoint.object.Capability" name="SHARED-Capability-Identity-READ"/>
        <Reference class="sailpoint.object.Capability" name="SHARED-Capability-Policies-READ"/>
        <Reference class="sailpoint.object.Capability" name="SHARED-Capability-Reports-READ"/>
        <Reference class="sailpoint.object.Capability" name="SHARED-Capability-Tasks-READ"/>
        <Reference class="sailpoint.object.Capability" name="SGT-Capability-CISOAllowedReports"/>
        <Reference class="sailpoint.object.Capability" name="SGT-Capability-ViewCertifications"/>
    </Capabilities>
    <Owner>
        <Reference class="sailpoint.object.Identity" name="spadmin"/>
    </Owner>
    <Preferences>
        <Map>
            <entry key="workgroupNotificationOption">
                <value>
                    <WorkgroupNotifationOption>Both</WorkgroupNotifationOption>
                </value>
            </entry>
        </Map>
    </Preferences>
</Identity>